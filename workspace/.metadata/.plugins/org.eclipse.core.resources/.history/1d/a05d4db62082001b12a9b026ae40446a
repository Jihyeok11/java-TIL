package day4;

class A{
	
	A a1 = new A();
	private A() {
		System.out.println("A 생성자 호출");
	}
	A getInstance() {
		return a1;
	}
	// private를 사용했으므로 A 클래스에서만 사용
//	A(){
//		System.out.println("A 생성자 호출");
//	}
}

public class SingletoneTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A a1 = new A();
		A a2 = a1;// A 타입 객체를 공유
		A a3 = new A(); // 
		
	}

}