package day2;

public class StarTest {

	public static void main(String[] args) {
		// TODO 5*5 사각형 모양 * 출력
		
//		for (int j=1; j<=5; j++) {
//			for (int i=1;i<=5;i++) {
//				System.out.print("*");
//				if (i==j) {
//					System.out.println();
//					break;
//				}
//			}
//		}
		
		char c = 'a';
		System.out.println((char)(++c));
		for(int i=1; i<=5; i++) {
			for(int j=1; j<=i; j++) {
				c++;
				System.out.print((char)(c));
				if (i==j) {
					break;
				}
			}
			System.out.println();
		}
	}
}
