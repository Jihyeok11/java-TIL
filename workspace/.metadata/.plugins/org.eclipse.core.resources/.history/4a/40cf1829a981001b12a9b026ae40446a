package day3;

class Student{
	String s = "";
	void name(String na) {
		s = na;
	}
	
}
public class StudentTest3 {

	public static void main(String[] args) {
		Student s = new Student();
		
		for(int i=0;i<args.length;i++) {
			System.out.println(args[i]);
			s.name = args[i];
			System.out.println("학생의 이름은 "+ s +"입니다");
		}

	}

}
