package day6;

class Multi1 extends Thread{
	public void run() {
		for(int i = 0; i<=5; i++) {
			System.out.println("다른 작업 수행");
		}
	}
}
public class MultiThreadTest {

	public static void main(String[] args) {
		Multi1 m1 = new Multi1();
		m1.start();
		for (int i = 1; i<= 5; i++) {
			System.out.println("메인 작업 수행");
		}
	}
	
}
